/* pages/zxla/sccl/sccl.wxss */
scroll-view{
  height:98%;
  background: #eeeeee;
}
/* 头部信息  */
.pageTitle{
   padding: 20rpx 50rpx;
} 
.step{
  display: inline-block
}

.num{
  width: 63rpx;
  height: 63rpx;
  background: #dfdfdf;
  border-radius: 50%;
  color:#888;
  text-align: center;
  line-height: 63rpx;
  font-size: 30rpx;
  margin: 0 auto;
  margin-bottom:10rpx;
}
.process{
  display: flex;
  justify-content: center;
  padding: 50rpx 0rpx;
  color:#888;
  background: #fff;
}
.process .line{
  margin-top: 4%;
  height:0;width: 120rpx;
  border-bottom:3rpx dashed #888;
}
.active-step{
  color:#333;  
}
.active-num{
  color:#fff;
  background: #2088d6;
  
}

/* 中间上传材料部分 */
.content{
  margin-top: 20rpx;
  background:#fff;
  padding-bottom:50rpx;
}

.content>view{
  width:90%;
  margin: 0 auto;
  border-bottom:2rpx solid #888; 
  padding: 25rpx 0rpx;
}
.content .title{
  margin-bottom: 20rpx;
}

.content .info{
  margin-left: 25rpx;
}

.info{
  display: flex;
  justify-content: flex-start;
}
.material image{
  height: 126rpx;
  width:126rpx;
}
.material{
  margin-right: 10rpx;
}

.title .name{
  color:#333;
  font-size: 26rpx;
}
.title .star{
  color:red;
}
.pic{
  display: flex;
  border:4rpx dashed #888;
  justify-content: center;
  align-items: center;
  height: 126rpx;
  width:126rpx;
  border-radius: 10rpx;
}
.pic image{
  width:62rpx;
  height:50rpx;
}
.content button{
  margin-top: 50rpx;
}