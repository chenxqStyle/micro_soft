.swiper-tab{
  width: 100%; 
  line-height: 100rpx;
  font-size: 36rpx;
  font-family: 微软雅黑;
}
.swiper-tab-list{
  font-size: 30rpx; 
  text-align: center; 
  display: inline-block; 
  width: 50%; 
  color: #777777; 
}
.on{
  color: #2088d6; 
  border-bottom: 5rpx solid #2088d6;
}
.swiper-box{ 
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
  background-color: #eeeeee;
 }
li.yyyg{
  color: black;
  display: block;
  margin-top: 15rpx;
  background-color: #fff;
}
.line {
  display: flex;
  border-bottom: 1rpx solid #e5e5e5;
  align-items:center;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.button {
  justify-content: center;
  border-bottom:none;
  height: 80rpx;
}
text {
  font-size: 26rpx;
}
.content {
  margin-left:30rpx;
}

li.yyjl {
  width:85%;
  display:flex;
  margin: 0 auto;
  margin-top:30rpx;
  background-color:#fff;
  border-radius: 8rpx;
  align-items:center;
  padding:30rpx 30rpx;
  justify-content: flex-start;
}

.record{
  width:98%;
}

.arrow{
  width: 5%;
  height:100%;
  text-align: center;
  padding:30rpx 5rpx;
}

.arrow image{
  width:25rpx;
  height:25rpx;
  display: inline-block;
}

.yy-title{
  color:#444;
  padding-left:20rpx;
  font-weight: bolder;
}

.yy-info{
  padding-left:60rpx;
}

.yy-title-content icon{
  vertical-align: bottom;
}
.yy-title-content{
  position: relative;
}
.yy-state{
  position: absolute;
  right: -10rpx;
  top:7rpx;
  background:#ffa200; 
  padding: 5rpx 10rpx;
  border-radius: 20rpx;
  color:#fefefe;
}

/*详情  */
.yy-detail{
  background: #fff;
  border-radius: 8rpx;
  width:85%;
  margin: 0 auto;
  padding:30rpx 30rpx;
  text-align: center;
  margin-top: 30rpx;
}

.yy-detail .ewm image {
  width:70%;
  display: block;
  margin:0 auto;
}

.detail-tittle{
  display: flex;
  /* margin: 30rpx 0rpx; */
  color:#444;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #777777;

}
.detail-tittle image{
  width:68rpx;
  height:68rpx;
  margin-left: 50rpx;
}

.detail-time{
  margin-left: 50rpx;
}
.ewm{
  margin-top: 60rpx;
}
.ewm text{
  display: inline-block;
  margin: 30rpx 0 100rpx 0;
}

.dst-nav{
  margin-bottom:60rpx;
}

.scroll-container{
  height:98%;
}
