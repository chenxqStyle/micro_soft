  height:100vh;
}

.mycaselist-per{
  display:flex;
  justify-content: center;
  flex-direction: column;
  padding:20rpx 0rpx;
  background: #fff;
  border-top:1px solid #e5e5e5;
  border-bottom:1px solid #e5e5e5;
}

.mycaselist-per view{
  display:flex;
  padding-left:30rpx;
  padding-right:30rpx;
  position: relative;
}

.mycaselist-per view:not(:first-child){
  margin-top:8rpx;
}

